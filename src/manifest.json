{
    "manifest_version": 2,
    "name": "qrCreater - 网址二维码一键生成工具",
    "version": "1.3.0",
    "permissions": ["tabs"],
    "description": "用于生成地址栏网址的二维码图片",
    "icons": {
        "128": "icon/qrcode.png",
        "32": "icon/qrcode.png"
    },
    "browser_action": {
        "default_icon": "icon/qrcode.png",
        "default_title": "qrCreater - 网址二维码一键生成工具",
        "default_popup": "index.html"
    }
} 